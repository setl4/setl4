.       iskind(kind,'map','set')   index(set)[r],
.       iskind(kind,'seq')         seq(set)[r],
.       iskind(kind,'str')         substr(text(set),r,1),
.       ident(datatype(set),'string') substr(set,r,1))
                                                    :(return)
arb.end

