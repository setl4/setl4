    out('key',key)
    out('next elem ',elem)
    show = (ident(show,'{') '{ ', show ' ') show(elem,,1)
    show = show ' '  show(elem,,1)
    lt(set.size(v),100)                             :s(show.set.ext)
    output = show
    show =                                          :(show.set.next)

show.set.done

    show = show ' }'
                                                    :(show.done)
show.str

    show = show.str(str.text(set))                  :(show.done)

show.string

*   Don't quote string that is an integer.

    show = integer(v) v                             :s(show.done)

    show = show.q(v)                                :(show.done)


show.table

    t = convert(v,'array')

show.table.next

