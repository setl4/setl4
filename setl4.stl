    this = next(set)                                :f(return)
    e = eval(expr)                                  :f(filter.loop)
    false(e)                                        :s(4.filter.loop)
    iskind(kind(set),'set') setl4.add(filter,this)  :(filter.loop)
    put(filter,key(this),value(this))           :(filter.loop)

filter.end

